{
  "name": "small-number",
  "version": "1.2.1",
  "description": "Easily turn large values into their corresponding number and letter value - 1,300,000 -> 1.3m",
  "main": "./lib/index.js",
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "scripts": {
    "build": "babel src -d lib",
    "prepublish": "npm run lint && npm run test && npm run build",
    "lint": "eslint src tests",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jxshco/small-number.git"
  },
  "keywords": [
    "number",
    "small number",
    "number conversion",
    "million",
    "javascript",
    "es6",
    "npm"
  ],
  "author": "Josh Sturrock <jxshco@gmai.com>",
  "bugs": {
    "url": "https://github.com/jxshco/small-number/issues"
  },
  "homepage": "https://github.com/jxshco/small-number#readme",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-preset-env": "^1.6.1"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
